<section class="top-list {{type}}">
    {{#with ctx}}
    <div>
        <h2>top {{../type}}</h2>
        <span>{{time_range}}</span>
    </div>
    <ol>
        {{#each list}} 
            <li>{{this}} {{#if ../paired}}<span>{{lookup ../paired @index}}</span>{{/if}}</li>
        {{/each}}
    </ol>
    {{/with}}
</section>

<section class="top-list {{type}}">
    {{#with ctx}}
    <div>
        <h2>top {{../type}}</h2>
        <span>{{time_range}}</span>
    </div>
    <ol>
        {{#each list.name}} 
            <li><a href="{{lookup ../list.external_urls_spotify @index}}" target="_blank">
                {{this}} 
                {{#if ../paired}}<span>{{lookup ../paired @index}}</span>{{/if}}
            </a></li>
        {{/each}}
    </ol>
    {{/with}}
</section>
